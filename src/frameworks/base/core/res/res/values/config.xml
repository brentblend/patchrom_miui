<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <integer name="expanded_status_bar_toggles_column">4</integer>
    <integer name="expanded_status_bar_toggles_land_column">4</integer>
    <integer name="expanded_status_bar_toggles_single_page_column">4</integer>
    <integer name="expanded_status_bar_toggles_single_page_land_column">6</integer>

    <!-- config for dirac -->
    <bool name="is_dirac_supported">false</bool>

    <!-- config for dolby -->
    <bool name="is_dolby_supported">false</bool>

    <!-- Enable USB mode selection -->
    <bool name="config_enableUsbModeSelection">false</bool>

    <!--  Has Antispam or not-->
    <bool name="config_has_antispam">true</bool>

    <!-- whether voice stream can be muted by phone -->
    <bool name="config_is_voice_mutable">false</bool>
    <bool name="config_screen_on_proximity_sensor_default">false</bool>

    <!-- configurations to control what components of status bar are showed under keyguard -->
    <bool name="config_show_status_bar_battery_for_keyguard">true</bool>
    <bool name="config_show_status_bar_carrier_for_keyguard">true</bool>
    <bool name="config_show_status_bar_notification_for_keyguard">true</bool>
    <bool name="config_show_status_bar_signal_for_keyguard">true</bool>
    <bool name="config_show_status_bar_status_for_keyguard">true</bool>
    <bool name="config_show_status_bar_time_for_keyguard">false</bool>
    <bool name="config_show_status_bar_carrier">false</bool>

    <bool name="enable_magnifier_when_input">false</bool>

    <fraction name="config_full_screen_expand_status_bar_height_ratio">50%</fraction>

    <fraction name="volume_panel_top_margin">27%</fraction>

    <!-- Default LED freq time for notification LED in milliseconds. -->
    <integer name="config_defaultNotificationLedFreq">4000</integer>
    <!-- Default value of ANTI_STRANGER_CALL_ENABLED -->
    <integer name="anti_stranger_call_enabled">1</integer>

    <!-- Default value of show rounded corners -->
    <integer name="config_show_rounded_corners_default">0</integer>

    <!-- config for mp3 record quality -->
    <integer name="config_mp3_record_quality">2</integer>

    <dimen name="window_background_blur_radius">15dp</dimen>
    <!-- Config for app's screenshot scale -->
    <item name="config_screenshot_scale" format="float" type="dimen">0.44444</item>

    <integer name="config_smartcover_smallwindow_left">90</integer>
    <integer name="config_smartcover_smallwindow_top">0</integer>
    <integer name="config_smartcover_smallwindow_right">990</integer>
    <integer name="config_smartcover_smallwindow_bottom">1180</integer>

    <!-- Config for button lights -->
    <integer name="config_buttonBrightnessSettingDefault">255</integer>

    <!-- List of packages which should have their notifications appear across
    all users. @hide -->
    <string-array translatable="false" name="config_crossSpaceNotificationPackageNames">
    </string-array>

</resources>
